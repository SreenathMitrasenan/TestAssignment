<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>
		<!-- report theme -->
		<!-- standard, dark -->
		<theme>standard</theme>

		<!-- enables timeline -->
		<!-- defaults to true -->
		<enableTimeline>true</enableTimeline>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>https</protocol>

		<!-- title of the document -->
		<documentTitle>Doorward GUI Test</documentTitle>

		<!-- report name - displayed at top-nav -->
		<reportName>
			<![CDATA[
            <img src='C:/Users/ellip/source/repos/DoorwardGUIAutomation/Configuration/img1.PNG'/>
        ]]>
		</reportName>

		<!-- custom javascript -->
		<scripts>
			<![CDATA[
        $(document).ready(function() {
        $(".brand-logo").hide(); 
        });
      ]]>
		</scripts>

		<!-- custom styles -->
		<styles>
			<![CDATA[
                .report-name { padding-left: 10px; } .report-name > img { float: left;height: 92%;margin-left: 0px;margin-top: 2px;width: auto; }
            ]]>
		</styles>
	</configuration>
</extentreports>